class Hotel {
    - String codigo
    - String nombre
    - String telefono
    - Integer plazasDisponibles
    - Boolean activo
    + getCodigo()
    + getNombre()
    + getTelefono()
    + getPlazasDisponibles()
    + getEstado()
    + setNombre()
    + setTelefono()
    + setEstado()
}





@enduml
