@startuml
class Turista {
    - String codigo
    - String nombre
    - String apellidos
    - String telefono
    - Boolean activo
    - Integer idCalle
    + set()
    + get()
}

class Sucursal {
    - Integer codigo
    - String telefono
    - Boolean activo
    + setTelefono()
    + setEstado()
    + getCodigo()
    + getTelefono()
    + getEstado()
}

class Vendedor {
    - String codigo
